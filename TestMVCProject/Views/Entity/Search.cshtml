@model IEnumerable<TestMVCProject.Models.Domain.EntityModel>

<h1>Search Results</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Age</td>
                <td><a href="Details/@item.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>